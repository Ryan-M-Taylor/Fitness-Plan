<!-- Loop over each object in the users array -->

{{#each users as |user|}}
<div class='row mb-4 workout main-container'>
  <div class='col-md-5'>
    <h2 class="user-name">
      <a href='/users/{{user.id}}'>{{user.name}}</a>
    </h2>
  </div>
  <div>
    <p class="total-workout-time"> Total Workout Time (hr): {{user.total_time}}</p>
  </div>
</div>
{{/each}}